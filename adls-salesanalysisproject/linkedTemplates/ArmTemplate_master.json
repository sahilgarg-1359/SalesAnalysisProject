{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adls-salesanalysisproject"
		},
		"ls_connectDatalakegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_connectDatalakegen2'"
		},
		"ls_getSaleasSqlData_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_getSaleasSqlData'"
		},
		"ls_connectDatalakegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stsalestransformdata.dfs.core.windows.net/"
		},
		"ls_databBricksNotebook_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0906-081047-9o2h97l9"
		},
		"ls_getSaleasSqlData_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-saleasanalysisdataenginnering.database.windows.net"
		},
		"ls_getSaleasSqlData_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-salesdata"
		},
		"ls_getSaleasSqlData_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SalesProject"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adls-salesanalysisproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_connectDatalakegen2_accountKey": {
						"value": "[parameters('ls_connectDatalakegen2_accountKey')]"
					},
					"ls_getSaleasSqlData_password": {
						"value": "[parameters('ls_getSaleasSqlData_password')]"
					},
					"ls_connectDatalakegen2_properties_typeProperties_url": {
						"value": "[parameters('ls_connectDatalakegen2_properties_typeProperties_url')]"
					},
					"ls_databBricksNotebook_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databBricksNotebook_properties_typeProperties_existingClusterId')]"
					},
					"ls_getSaleasSqlData_properties_typeProperties_server": {
						"value": "[parameters('ls_getSaleasSqlData_properties_typeProperties_server')]"
					},
					"ls_getSaleasSqlData_properties_typeProperties_database": {
						"value": "[parameters('ls_getSaleasSqlData_properties_typeProperties_database')]"
					},
					"ls_getSaleasSqlData_properties_typeProperties_userName": {
						"value": "[parameters('ls_getSaleasSqlData_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}